<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 <configuration>
 	<typeAliases>
 		<typeAlias type="com.pet.spring.users.dto.UsersDto" alias="usersDto"/>
 		<typeAlias type="com.pet.spring.review.dto.ReviewDto" alias="reviewDto"/>
 		<typeAlias type="com.pet.spring.review.dto.ReviewCommentDto" alias="reviewCommentDto"/>
 		<typeAlias type="com.pet.spring.adopt.dto.AdoptDto" alias="adoptDto"/>
 		<typeAlias type="com.pet.spring.adopt.dto.AdoptLikeDto" alias="adoptLikeDto"/>
 		<typeAlias type="com.pet.spring.adopt.dto.ReserveDto" alias="reserveDto"/>
 	</typeAliases>
 	
 	<mappers>
 		<mapper resource="com/pet/spring/mybatis/UsersMapper.xml"/>
 		<mapper resource="com/pet/spring/mybatis/ReviewMapper.xml" />
 		<mapper resource="com/pet/spring/mybatis/ReviewCommentMapper.xml" />
 		<mapper resource="com/pet/spring/mybatis/MemberMapper.xml"/>
 		<mapper resource="com/pet/spring/mybatis/AdoptMapper.xml"/>
 		<mapper resource="com/pet/spring/mybatis/AdoptLikeMapper.xml"/>
 		<mapper resource="com/pet/spring/mybatis/ReserveMapper.xml"/>
  </mappers>

 </configuration>